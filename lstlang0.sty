\lst@definelanguage{PRA}%
  {morekeywords={%
	% Operators
      SELECT,PROJECT,JOIN,UNITE,SUBTRACT,BAYES,%
      LEFTJOIN,RIGHTJOIN,INNERJOIN,OUTERJOIN,MULTIPLY,%
      NATURALJOIN,%
