@inproceedings{Fuhr:98a,
  author={Fuhr, N.},
  title={From Uncertain Inference to Agent-Based Information Retrieval},
  booktitle={VDI'98 - Arbeitstagung der GI-Fachgruppen Verteilte K\"unstliche
    Intelligenz, Datenbanken und Information Retrieval},
  language=DE,
  year=1998,
  note={http://ls6-www.cs.uni-dortmund.de/ir/reports/98/Fuhr-98a.html},
  abstract={  The logical approach to information retrieval (IR) treats
    retrieval as uncertain inference. In advanced applications, we
    have to deal   with a multi-step inference process involving
    different system   components: information needs, search
    activities, queries, query   intermediaries, databases and
    documents. In order to provide a   better system support for
    satisfying a user's information need,   these components should be
    changed into active objects (agents) with   high-level
    functions. Especially, we describe different levels of   search
    activities (moves, tactics, stratagems and strategies) as   well
    as different types of documents (data only, objects with IR
    methods, active objects, communicating objects).}}
