@inproceedings{Roelleke/Fuhr:97,
  author={Roelleke, T. and  Fuhr, N.},
  title={Probabilistic Reasoning for Large Scale Databases},
  editor={Dittrich, K.R. and  Geppert, A.},
  booktitle={Datenbanksysteme in B\"uro, Technik und Wissenschaft (BTW'97)},
  publisher={Springer},
  address={Berlin et al.},
  pages={118-132},
  language=EN,
  year=1997,
  abstract={  The complexity of probabilistic reasoning prohibits its
    application on a large scale of data.  In order to reduce the
    complexity,  implementations of modeling approaches restrict
    themselves with   respect to expressive power or relax on the
    underlying probability   theory.      We present the
    implementation aspects of a probabilistic extension   of
    stratified Datalog.  This probabilistic deductive system is
    strictly based on the well-founded ground of probability theory.
    The prototypical implementation of the system handles the
    expensive   computation of the probabilities separately from the
    reasoning   process itself.  Thus, we can use standard
    optimization strategies   known from deterministic systems in
    order to cope with large amounts   of data.}}
