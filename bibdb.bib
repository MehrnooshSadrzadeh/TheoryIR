@inproceedings{Fuhr:92b,
  author={Fuhr, N.},
  title={Integration of Probabilistic Fact and Text Retrieval},
  editor={Belkin, N. and  Ingwersen, P. and  Pejtersen, M.},
  booktitle={Proceedings of the Fifteenth Annual International {ACM SIGIR}
    Conference on Research and Development in Information Retrieval},
  publisher=ACM,
  address={New York},
  pages={211-222},
  language=EN,
  year=1992,
  abstract={In this paper, a model for combining text and fact retrieval is
    described.  A query is a set of conditions, where a single
    condition is either a text or fact condition.  Fact conditions can
    be interpreted as being vague, thus leading to nonbinary weights
    for fact conditions with respect to database objects.  For text
    conditions, we use descriptions of the occurrence of terms in
    documents instead of precomputed indexing weights, thus treating
    terms similar to attributes.  Probabilistic indexing weights for
    conditions are computed by introducing the notion of correctness
    (or acceptability) of a condition w.r.t.\ an object.  These indexing
    weights are used in retrieval for a probabilistic ranking of
    objects based on the retrieval-with-probabilistic-indexing (RPI)
    model, for which a new derivation is given here.}}
@inproceedings{Fuhr:93,
  author={Fuhr, N.},
  title={Representations, Models and Abstractions in
    Probabilistic Information Retrieval},
  editor={Opitz, O. and  Lausen, B. and  Klar, R.},
  booktitle={Information and Classification. Concepts, Methods and Applications},
  publisher={Springer},
  address={Berlin et al.},
  pages={259-267},
  language=EN,
  year=1993,
  abstract={We show that most approaches in probabilistic information
    retrieval can be regarded as a combination of the three concepts
    representation, model and abstraction.  First, documents and
    queries have to be represented in a certain form, e.g.  as a sets
    of terms.  Probabilistic models use certain assumptions about the
    distribution of the elements of the representation in relevant and
    nonrelevant documents in order to estimate the probability of
    relevance of a document w.r.t.  a query.  Older approaches based
    on query-specific relevance feedback are restricted to simple
    representations and models.  Using abstractions from specific
    documents, terms and queries, more powerful approaches can be realized.}}
@inproceedings{Fuhr:93a,
  author={Fuhr, N.},
  title={A Probabilistic Relational Model for the Integration of IR and
    Databases},
  booktitle={Proceedings of the Sixteenth Annual International {ACM SIGIR}
    Conference on Research and Development in Information Retrieval},
  publisher=ACM,
  address={New York},
  pages={309-17},
  language=EN,
  year=1993,
  abstract={In  this  paper,  a  probabilistic  relational  model  is
    presented which  combines  relational  algebra  with
    probabilistic  retrieval. Based  on  certain  independence
    assumptions,  the  operators  of the  relational  algebra  are
    redefined  such  that  the  probabilistic algebra  is  a
    generalization  of  the  standard  relational
    algebra. Furthermore,  a  special  join  operator  implementing
    probabilistic retrieval  is  proposed.    When  applied  to
    typical  document  databases, queries  can  not  only  ask  for
    documents,  but  for  any  kind  of object  in  the  database.
    In  addition,  an  implicit  ranking  of these  objects  is
    provided  in  case  the  query  relates  to  probabilistic
    indexing  or  uses  the  probabilistic  join  operator.    The
